[?2004l#ifndef _CELL_H

#define _CELL_H

#include <stdlib.h>
#include <inttypes.h>

#define NAME_SIZE 20
#define GROUP_SIZE 15
#define MARKS_COUNT 4

#define TABLENAME_LENGTH 20

typedef struct mark {
    int8_t type; // –ó–∞—á–µ—Ç/—ç–∫–∑–∞–º–µ–Ω - 0/1 
    int8_t mark;  // –û—Ü–µ–Ω–∫–∞ (–æ—Ç 2 –¥–æ 5, –µ—Å–ª–∏ —ç–∫–∑–∞–º–µ–Ω –∏ 0 –∏–ª–∏ 1, –µ—Å–ª–∏ –∑–∞—á–µ—Ç)

} mark;

typedef struct cell {
    char NAME[NAME_SIZE];     // –§–ò–û
    int8_t sex;               // –ü–æ–ª
    char group[GROUP_SIZE];   // –ù–æ–º–µ—Ä –≥—Ä—É–ø–ø—ã
    mark marks[MARKS_COUNT];  // –û—Ü–µ–Ω–∫–∏ –ø–æ –ø—Ä–µ–¥–º–µ—Ç–∞–º:
                              // –ê–°–î, –î–∏—Å–∫—Ä–µ—Ç–Ω–∞—è –º–∞—Ç–µ–º–∞—Ç–∏–∫–∞, 
